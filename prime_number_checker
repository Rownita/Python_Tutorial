number = int(input("Enter the number: "))

def is_prime(num):
    for i in range(2,number):
        if number%i==0 : ###if the number is divisible then it is not prime
            return False
            break
        
    
    return True
         
result=is_prime(number)

if result== True:
    print("The number is prime")
else:
    print("The number is not prime")
    
