new_list = ["superman","batman","spiderman","ratman"]

new_list.sort()
print(new_list)

new_list = ["Banana","orange","blueberry","Grapes"]

new_list.sort() ### case sensitive. sort() method sort from uppercase to lowercase
print(new_list)

list2= [1,2,8,9,11,100,87,45]

list2.sort() ###sorting in ascending order
print(list2)

list3 = [49,7,8,10,20,4]
list3.sort(reverse=True)##sorting in descending order
print(list3)
