this_list = ["apple","banana","cherry","banana"]

this_list.remove("banana") ###removing list item

print(this_list)
this_list.pop(1)##removing 2nd element
print(this_list)

new_list = ["batman","superman","spiderman","ratman"]

new_list.pop()
print(new_list)
new_list.clear()

del new_list[1]
del new_list

    
