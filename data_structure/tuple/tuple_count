###counting element of tuple
t = (1,2,3,2,4,2)
print(t.count(2))
print(t.count(3))

tuple2 = ("apple","banana","cherry","apple")
print(tuple2.count("apple"))

print(tuple2.count("cherry"))

tuple3= (10,20,30,20,30)
print(tuple3.count(20))


tuple3= (10,20,30,20,30)
print(tuple3.count(20))

print("index: ",tuple3.index(30))
